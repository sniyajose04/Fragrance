                                            <img src="/productImages/<%= products[i].images[0].filename %>" alt="#">
                                        </div>
                                        <div class="content pt-10">
                                            <h5><a href="productDetail?id=<%=products[i]._id %>"><%= products[i].product_title %></a></h5>
                                            <div class="product-price">
                                                <span>&#8377;<%=products[i].offerPrice>0 ?products[i].offerPrice : products[i].promotionalPrice %> </span>
                                                
                                               
                                               
                                            </div>
                                            
                                        </div>
                                    </div>
                                <% } %>
                            <% } else { %>
                                <p>No new products available.</p>
                            <% } %>
                       
                    
                </div>
            </div>
        </section>
    </main>
